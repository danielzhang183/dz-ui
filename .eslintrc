{
  "extends":["@antfu"],
  "rules": {
    "yml/no-empty-document": "off"
  },
  "overrides": [
    {
      "files": [
        "playground/**/*.*"
      ],
      "rules": {
        "no-restricted-imports": "off"
      }
    },
    {
      "files": [
        "**/*.md/*.*"
      ],
      "rules": {
        "no-restricted-imports": "off",
        "no-restricted-syntax": "off",
        "no-labels": "off",
        "@typescript-eslint/no-unused-vars": "off",
        "@typescript-eslint/no-var-requires": "off"
      }
    }
  ]
}
